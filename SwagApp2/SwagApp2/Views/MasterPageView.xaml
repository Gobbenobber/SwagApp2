<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="SwagApp2.Views.MasterPageView"
             Title="MasterPageView"
             Padding="0,0,2,0"
             BackgroundColor="Black"
             >

    <StackLayout BackgroundColor="Red" Padding="40" Spacing="15">
        <Button Text="{Binding NavigateToListViewTxt}" Style="{StaticResource ButtonStyle}" Command="{Binding NavigateCommand}" CommandParameter="MasterDetailPageView/BaseNavigationPageView/ListPageView"/>
            <Button Text="{Binding NavigateToStatisticsTxt}" Style="{StaticResource ButtonStyle}"/>
        </StackLayout>

</ContentPage>